        "heading_HT3c6U": {
          "type": "heading",
          "settings": {
            "heading": "Informationen für Verbraucher bei Fernabsatzverträgen sowie Kundeninformationen bei Verträgen im elektronischen Geschäftsverkehr",
            "heading_size": "h6",
            "heading_h1": false
          }
